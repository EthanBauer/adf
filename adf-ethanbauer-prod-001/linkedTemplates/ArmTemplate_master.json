{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-ethanbauer-prod-001"
		},
		"ethanbauerserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ethanbauerserver'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=ethanbauer-server-prod-001.database.windows.net;Initial Catalog=ethanbauerDB-prod-001;User ID=cloudadmin"
		},
		"AlphaVantageAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.alphavantage.co"
		},
		"DownloadStockListHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.alphavantage.co/query?function=LISTING_STATUS&apikey=BURCSC12I2PSYHNS"
		},
		"KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-ethanbauer.vault.azure.net/"
		},
		"ethanbauerdatalakeLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ethanbauerdatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-ethanbauer-prod-001_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ethanbauerserver_connectionString": {
						"value": "[parameters('ethanbauerserver_connectionString')]"
					},
					"AlphaVantageAPI_properties_typeProperties_url": {
						"value": "[parameters('AlphaVantageAPI_properties_typeProperties_url')]"
					},
					"DownloadStockListHTTP_properties_typeProperties_url": {
						"value": "[parameters('DownloadStockListHTTP_properties_typeProperties_url')]"
					},
					"KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('KeyVault_properties_typeProperties_baseUrl')]"
					},
					"ethanbauerdatalakeLinkedService_properties_typeProperties_url": {
						"value": "[parameters('ethanbauerdatalakeLinkedService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}